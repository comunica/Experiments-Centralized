{
  "private": true,
  "name": "bsbm",
  "dependencies": {
    "@jbr-experiment/bsbm": "^5.4.0",
    "@jbr-hook/docker": "^5.4.0",
    "@jbr-hook/sparql-endpoint-comunica": "^5.4.0",
    "cross-env": "^7.0.3",
    "jbr": "^5.4.0"
  },
  "scripts": {
    "jbr": "cross-env NODE_OPTIONS=--max-old-space-size=8192 jbr",
    "validate": "jbr validate",
    "plot": "psbr csv bsbm combinations/combination_*/output && npm run plot-small && npm run plot-large",
    "plot-small": "psbr tex query combinations/combination_*/output -n 'plot_small' -q '^[^5]' --svg --overrideCombinationLabels Comunica-Memory,Comunica-HDT,Oxigraph-Rust,Oxigraph-JS,Oxigraph-JS-RDFJS,Jena",
    "plot-large": "psbr tex query combinations/combination_*/output -n 'plot_large' -q '^5' --svg --overrideCombinationLabels Comunica-Memory,Comunica-HDT,Oxigraph-Rust,Oxigraph-JS,Oxigraph-JS-RDFJS,Jena"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
